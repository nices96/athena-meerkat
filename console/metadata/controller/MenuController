                    "    var domainId = parentId.substr(parentId.lastIndexOf(\"_\") + 1);",
                    "    params = {\"domainId\":domainId};",
                    "    url+=\"/domain/tomcatlist\";",
                    "    is_child_leaf = true;",
                    "    prefix_child_menu_id = parentId + \"_tomcat_\";",
                    "",
                    "}",
