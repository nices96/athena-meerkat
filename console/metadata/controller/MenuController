                    "           switch (_item.id) {",
                    "                case 'new-domain':",
                    "                    webapp.app.getController(\"DomainController\").showDomainWindow(\"new\", 0);",
                    "                    break;",
                    "                case 'collapse':",
                    "                    //item.collapse();",
                    "                    break;",
                    "                case 'refresh':",
                    "                    webapp.app.getController(\"MenuController\").loadChildMenus(menuId);",
                    "                    break;",
                    "                default:",
                    "                    webapp.app.getController(\"MenuController\").loadChildMenus(menuId);",
                    "                    break;",
                    "           }",
                    "        },",
